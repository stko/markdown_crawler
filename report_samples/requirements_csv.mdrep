outputdir:.
outputfilemask:$n_csv.txt

<% for name, obj in self.objs.items():
 obj=obj['obj']
 if 'requirements' in obj:
  for req in obj['requirements']:
   if isinstance(req,dict) and 'text' in req:%>
    <indent>
        "<%= name %>","<%= " ".join(req['text']).strip().replace('"','""') %>"<%= '\n' %>
    </indent>

